{
  "server": {
    "accessLog": "/var/log/supervisor/access.log",
    "port": 80,
    "workers": 10,
    "maxSockets": 100,
    "deadBackendTTL": 30,
    "tcpTimeout": 30,
    "retryOnError": 3,
    "deadBackendOn500": true,
    "httpKeepAlive": false
  },
  "driver": [ "redis://127.0.0.1:6379" ]
}
